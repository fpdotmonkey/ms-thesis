@Article{Sinha2021,
  author        = {Sinha, Roopak and Pang, Cheng and Martínez, Gerardo Santillán and Kuronen, Juha and Vyatkin, Valeriy},
  journal       = {Proceedings of the 20th International Conference on Engineering of Complex Computer Systems (ICECCS2015). Gold Coast, QLD, Australia, IEEE Computer Society Press, pp.198-201},
  title         = {Requirements-Aided Automatic Test Case Generation for Industrial Cyber-physical Systems},
  year          = {2021},
  month         = aug,
  abstract      = {Industrial cyber-physical systems require complex distributed software to orchestrate many heterogeneous mechatronic components and control multiple physical processes. Industrial automation software is typically developed in a model-driven fashion where abstractions of physical processes called plant models are co-developed and iteratively refined along with the control code. Testing such multi-dimensional systems is extremely difficult because often models might not be accurate, do not correspond accurately with subsequent refinements, and the software must eventually be tested on the real plant, especially in safety-critical systems like nuclear plants. This paper proposes a framework wherein high-level functional requirements are used to automatically generate test cases for designs at all abstraction levels in the model-driven engineering process. Requirements are initially specified in natural language and then analyzed and specified using a formalized ontology. The requirements ontology is then refined along with controller and plant models during design and development stages such that test cases can be generated automatically at any stage. A representative industrial water process system case study illustrates the strengths of the proposed formalism. The requirements meta-model proposed by the CESAR European project is used for requirements engineering while IEC 61131-3 and model-driven concepts are used in the design and development phases. A tool resulting from the proposed framework called REBATE (Requirements Based Automatic Testing Engine) is used to generate and execute test cases for increasingly concrete controller and plant models.},
  archiveprefix = {arXiv},
  copyright     = {arXiv.org perpetual, non-exclusive license},
  doi           = {10.1109/ICECCS.2015.32.},
  eprint        = {2108.07400},
  file          = {:http\://arxiv.org/pdf/2108.07400v1:PDF},
  keywords      = {Software Engineering (cs.SE), FOS: Computer and information sciences},
  primaryclass  = {cs.SE},
  publisher     = {arXiv},
}

@MastersThesis{Naervaenen2022,
  author   = {Ville Närvänen},
  school   = {Aalto University},
  title    = {Implementation of dynamic loadsystem in direct driven hydraulics testbench},
  year     = {2022},
  month    = apr,
  type     = {mathesis},
  abstract = {In off-road mobile machinery, hydraulics are commonly used due to their high power-to-weight ratio, robustness, and affordability. Major disadvantage of traditional hydraulic systems is their generally poor energy efficiency, which increases emissions and increases operational costs. Also, in electric mobile machinery, operational times are negatively affected due to limited battery power. Such issues are major motivators for development of more energy efficient solutions in recent years.
Typical research methods for novel hydraulic systems includes test bench and simulation of studied system. Experimental test bench is typically loaded with either mass or load cylinder, capable of generating static loads. However, due to work cycles in field usage, hydraulic systems are almost never statically loaded. Therefore, data generated by statically loaded test bench is not directly comparable to field usage. Ideally, test benches would include load system capable of inducing loads similar to field usage, which could produce energy efficiency results relevant to actual field operation.
In this research dynamic load system was designed and implemented to direct driven hydraulic (DDH) test bench simulating mine loaders articulated steering system. Load system was tested with dynamic load reference simulating field usage and several static load situations to test system performance with high pressure and speed. System was first simulated before implementation and experimental testing were performed.
Target values for static and dynamic load situations were defined. In dynamic loading, target was for measured pressure, integrated respect to time, to stay within 5 percentage points of integrated reference pressure. In static loading, pass ranges were defined as within 5 percentage points of target value for each test. Load system could perform dynamic loading in DDH circuit following reference profile with 93.4% accuracy, which fell 1.6 percentage points short of target value of 95%. Static loading tests stayed within defined target values until after 100 mm/s actuation speed and 200 bar pressure thresholds were surpassed in steering system. In simulation environment all test stayed within target values. Overall, load system showed clear functionality as intended in both cases, although not all target values were reached. Improvements to load system control and architecture were suggested.},
  file     = {:Naervaenen2022 - Implementation of Dynamic Loadsystem in Direct Driven Hydraulics Testbench.pdf:PDF:https\://aaltodoc.aalto.fi/server/api/core/bitstreams/af8d397b-0160-40cb-b0e1-0affe132c06f/content},
  url      = {https://aaltodoc.aalto.fi/items/4e823292-6147-4892-b6c1-1067f5a5a98e},
}

@MastersThesis{Hermansson2021,
  author   = {Robert Hermansson},
  school   = {Aalto University},
  title    = {Testing and simulation of energy efficiency of direct drive hydraulic system},
  year     = {2021},
  month    = jul,
  type     = {mathesis},
  abstract = {Because of the tightening emission regulations, reduction of the energy consumption is one of the main development goals of mobile work machinery. Electrification and hybridization are key methods in this process. In mobile work machinery often major amount of the engine’s mechanical power is first converted to hydraulic, which is transmitted and converted back to mechanical power of the actuators. For this reason, the energy efficiency of the hydraulic system has a significant impact on the energy consumption of the machinery.
Usually, the energy efficiencies of hydraulic systems are low compared to mechanical or electro-mechanical alternatives. This is mainly because of the throttling losses in directional control valves, where the energy is dissipated and converted into heat. In direct drive hydraulic system, the hydraulic actuator is directly controlled with the rotational speed and direction of electrically operated hydraulic pump. This eliminates the throttling losses in control valves enhancing the energy efficiency of the system.
In this research the energy efficiency of a direct drive hydraulic system was measured and simulated. The measurements were executed in a test bench which was used for load lifting and lowering. Three different loads and velocities were used to study how they affect the energy efficiency. Besides the experimental measurements, a simulation model of the system was created. The goal was to create a realistic model which can be used for energy efficiency analysis and future research work with the system.
The load and velocity had a remarkable effect on system’s energy efficiency. The best total efficiency of the system was 82 % in load lifting cycle of lowest velocity and highest load.  During the same cycle, 79 % of system’s mechanical power was available for regeneration.  The worst total efficiency of the system was 44 % with lowest load and highest velocity. This cycle also required positive input power from the servomotor during load lowering. The differences between simulated and measured results were 0…5 % in most cycles.
The measurement results revealed a remarkable energy saving potential of direct drive hydraulic system. The efficiency’s strong dependency on load and velocity shows the importance of component sizing according to the operational requirements. While the typical energy efficiencies of conventional hydraulic systems are around 35 %, the direct drive hydraulics would allow remarkable energy savings in mobile work machinery.},
  file     = {:Hermansson2021 - Testing and Simulation of Energy Efficiency Ofdirect Drive Hydraulic System.pdf:PDF:https\://aaltodoc.aalto.fi/server/api/core/bitstreams/646c50a8-2137-4c8e-a545-67023f4dc1f4/content},
  url      = {https://aaltodoc.aalto.fi/items/bf6b77e2-3f44-4690-8cd3-03a166b1e942},
}

