@InProceedings{Chen2022,
  author          = {Haocong Chen and Yutao Chen and Wu Wang and Dingci Lin and Jie Huang},
  title           = {A Dynamic Safe Arbitrator for Human-Machine Shared Control},
  year            = {2022},
  address         = {Xiamen, China},
  pages           = {3395--3400},
  publisher       = {IEEE},
  abstract        = {Shared control has become a popular subject of human-machine hybrid intelligence. This paper presents a dynamic safety arbitrator for shared control in an automated vehicle. The vehicle is controlled autonomously by a model predictive controller (MPC) tracking a given trajectory, while at the same time human input is provided via a joystick. Unlike traditional mixed-initiative control methods that blend weighted human and vehicle inputs, the proposed arbitrator first adopts control barrier function (CBF) to generate weights for human and vehicle inputs under safety critical situations, and then employs the null-space based control (NSBC) to merge the two inputs with priorities determined by the weights. As a result, the final input to the vehicle has a clear physical meaning that represents dynamic priorities of human and vehicle autonomous inputs, in which the higher priority input is fully adopted and the lower priority input is partially adopted without intervening the higher priority input. An immediate advantage of the proposed arbitrator is that there is no need to tune the weights between human and autonomous inputs. Simulation results show that the dynamic safe arbitrator is effective for shared control.},
  date            = {25-27 Nov. 2022},
  doi             = {10.1109/CAC57257.2022.10055010},
  eventdate       = {25-27 Nov. 2022},
  eventtitleaddon = {Xiamen, China},
  file            = {:- A Dynamic Safe Arbitrator for Human Machine Shared Control.pdf:PDF},
  isbn            = {978-1-6654-6534-2},
  issn            = {2688-092X},
  journal         = {2022 China Automation Congress (CAC)},
  keywords        = {Automation, Decision making, Predictive models, Real-time systems, Human in the loop, Safety, Trajectory, human-machine collaboration, shared control, automated driving},
  readstatus      = {read},
}

