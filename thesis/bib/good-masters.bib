  issn            = {2832-188X},
  journal         = {2023 9th International Conference on Control Science and Systems Engineering (ICCSSE)},
  keywords        = {Temperature distribution, Simulation, Optimal control, Reinforcement learning, Systems engineering and theory, Temperature control, Safety, Aeroengine, Safe Reinforcement Learning, Barrier Function, Temperature Limited, Optimum Control},
}

