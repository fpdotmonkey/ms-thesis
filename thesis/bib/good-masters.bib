  isbn            = {978-1-61284-342-1},
  journal         = {2011 IEEE International Symposium on Assembly and Manufacturing (ISAM)},
  keywords        = {Robot kinematics, Programming, Control systems, Kinematics, Service robots, Delta modulation, Modular Control System, Reconfigurable Assembly System, Multiple Arm Robotic System},
  readstatus      = {read},
}

@InProceedings{Jiang2022,
