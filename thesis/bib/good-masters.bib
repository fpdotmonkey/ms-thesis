@Article{Dudzinski1989,
  author    = {Dudziński, Piotr A.},
  journal   = {Journal of Terramechanics},
  title     = {Design characteristics of steering systems for mobile wheeled earthmoving equipment},
  year      = {1989},
  issn      = {0022-4898},
  month     = jan,
  number    = {1},
  pages     = {25--82},
  volume    = {26},
  abstract  = {This paper discusses a systematic classification of design and operations parameters, typical of steering systems of mobile earthmoving equipment, in respect of various steering gears and the main elements of a steering unit. The basic advantages and disadvantages of these methods are compared. Additionally, the steering gears of articulated frame-steered vehicles have been chosen to exemplify the method for determining all possible designs of level mechanisms. Furthermore, by means of a polyoptimization method, the optimum geometry of a steering mechanism has been established in view of design and operational requirements. Such a procedure is more advantageous than that of common designing practice nowadays which relies frequently either on using and adapting the well known solutions or analysing some random concepts. The polyoptimization method discused in this paper, in comparison with the most frequent design methods with one target function, provides a designer with more realistic results. The new solutions obtained through polyoptimization and synthesis have preferably design and operational parameters. The method discussed can also be successfully used in other areas of technology.},
  comment   = {Background on articulated steering re: Dolores},
  doi       = {10.1016/0022-4898(89)90025-6},
  file      = {:Dudzinski1989 - DESIGN CHARACTERISTICS OF STEERING SYSTEMS FOR MOBILE WHEELED EARTHMOBING EQUIPTMENT.pdf:PDF},
  publisher = {Elsevier BV},
}

@InProceedings{Lodewyks2016,
  author    = {Johann Lodewyks and Pascal Zurbrügg},
  booktitle = {10th International Fluid Power Conference},
  title     = {Decentralized energy-saving hydraulic concepts for mobile working machines},
  year      = {2016},
  abstract  = {The high price of batteries in working machines with electric drives offer a potential for investment  in  energy-saving  hydraulic  systems.  The  decentralized  power  network  opens up new approaches for hydraulic- and hybrid circuits. In addition, the regenera-tion of energy can be used at any point of the machine. For the example of an excava-tor arm drive with a double cylinder two compact hydraulic circuits are presented, which relieve a central hydraulic system.},
}

