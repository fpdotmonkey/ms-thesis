@Article{Barling1991,
  author   = {G. Barling},
  title    = {The Changing World of Avionics},
  year     = {1991},
  abstract = {The origins of GEC Avionics (GAv) have two distinct roots, Elliott and Marconi, both of which are historically significant. Table 1 shows the early major nlilestones in the company's history. The company can trace its ongIns right back to William Elliott who set up as an instrument maker in 1800. Elliotts were equipping aircraft with instruments in 1909, but even before that pocket altimeters, similar to that shown in Figure 1, were being nlanufactured, and may well have been used by balloonists. A gentleman balloonist would only have had to attach his altimeter to his watch chain for safe keeping! Figure 2 shows an advertisenlent for Elliott Flying Instruments in Flight magazine (now Flight International» of 20 November 1909. Typical of those instruments are the airspeed indicator, altimeter and inclinometer shown in Figure 3, with which the Vickers Gunbus was equipped. The Marconi Company pioneered wireless telegraphy, the foundation of modern radio. As mentioned in Table 1, Marconi equipment was used in aircraft-ta-ground communications tests in 1909 and Figure 4 is a photograph taken during such a test. During the test programme, the transmitted signal was received intelligibly at a distance of 400 m. By 1913 the Marconi Aeronautical Division was able to offer for sale their type AD2 wireless which is illustrated in Figure 5. In modern terms the AD2 system would be described as comprising 6 line-replaceable units (LRUs) plus interconnecting cables. Note also that one of the LRUs is an airstream driven generator. It is interesting to note that, to this day, the type numbers of airborne radio products of GEC Sensors are prefixed with the letter's 'AD'. The present-day companies of GEC Avionics, based at Rochester, and GEC Sensors, based at Basildon, are the direct descendants of the old Elliott and Marconi companies respectively with a cOlnbined product range encompassing a major part of the total avionics field.},
  doi      = {10.1177/002029409102400304},
  url      = {https://www.semanticscholar.org/paper/be870941357fef22b0dbbe20efc8ffb1d36ada84},
}

@Article{Reddy2022,
  author   = {C. Reddy},
  title    = {HISTORY OF AVIATION AND AIRLINE MARKETING},
  year     = {2022},
  abstract = {The term 'aeronautics' is most generally used to portray mechanical air transportation, which is completed utilizing an airplane. The two fundamental kinds of airplane are planes and helicopters, yet mos t present day meanings of the word 'fligh reach out past this to incorporate the utilization of automated airplane, like robots. Considering this, the avionics business can be portrayed as all industry that encompasses these exercises. Flying started in the eighteenth hundred years with the turn of events of the sight-seeing balloon, a contraption fit for barometrical removal through lightness. The absolute most huge headways in aeronautics innovation accompanied the controlled coasting flying of Otto Lilienthal in 1896; then, at that point, a huge advance in importance accompanied the development of the first fueled plane by the Wright siblings in the mid 1900s. Since that time, flying has been innovatively changed by the presentation of the stream which allowed a significant type of transport all through the world.},
  doi      = {10.55041/ijsrem13058},
  url      = {https://www.semanticscholar.org/paper/586b415d7e20326d7b02ea34b56781893f3533dc},
  venue    = {INTERANTIONAL JOURNAL OF SCIENTIFIC RESEARCH IN ENGINEERING AND MANAGEMENT},
}

